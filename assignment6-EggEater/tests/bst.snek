(fun (element_in_the_tree head val)
    (let ((node head))
        (loop
            (if (= val (index node 0))
                (break true)
                (if (> val (index node 0))
                    (if (index node 2)
                        (set! node (index node 2))
                        (break false)
                    )
                    (if (index node 1)
                        (set! node (index node 1))
                        (break false)
                    )
                )
            )
        )
    )
)

